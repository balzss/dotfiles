Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: none
Xft.autohint: false
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

URxvt.scrollBar:           false
URxvt.saveLines:           8192
URxvt.geometry:             800x300
URxvt.highlightSelection:  true
URxvt.loginShell:          true
URxvt.termName:            xterm-256color
URxvt.letterSpace:         -1
URxvt.lineSpace:            1
URxvt.font:                xft:Source Code Pro-16
URxvt.boldFont:            xft:Source Code Pro-16
