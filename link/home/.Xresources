! rofi
rofi.modi: run
rofi.font: DejaVu Sans Mono for Powerline 18
rofi.width: 60
rofi.lines: 8
rofi.columns: 1
!                    bg       fg      altbg    hlbg     hlfg
rofi.color-normal: #282828, #ebdbb2, #282828, #ebdbb2, #282828
rofi.color-urgent: #282828, #ebdbb2, #282828, #ebdbb2, #282828
rofi.color-active: #282828, #ebdbb2, #282828, #ebdbb2, #282828
rofi.color-window: #282828, #458588, #458588
rofi.bw: 3
! location: 1 2 3 / 8 0 4 / 7 6 5
rofi.location: 0
rofi.padding: 10
rofi.fuzzy: true
rofi.separator-style: solid
rofi.hide-scrollbar: true
