full_text=|
separator=false
separator_block_width=6
color=#ebdbb2

[updates]
command=$HOME/scripts/updates.sh
interval=60
signal=3

[volume]
label=vol:
command=$HOME/scripts/volume -i
interval=once
signal=2

[separator]

[wireless]
label=net:
command=$HOME/scripts/network
interval=10

[separator]

[battery]
label=pow:
command=$HOME/scripts/battery
interval=120

[separator]

[time]
command=date '+%a %d %b - %H:%M '
interval=10
