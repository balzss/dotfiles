URxvt.scrollBar:           false
URxvt.saveLines:           8192
URxvt.geometry:             256x96
URxvt.highlightSelection:  true
URxvt.loginShell:          true
URxvt.termName:            xterm-256color
URxvt.letterSpace:         -1
URxvt.font:                xft:DejaVu Sans Mono-13:antialias=true:autohint=true
URxvt.boldFont:            xft:DejaVu Sans Mono-13:antialias=true:autohint=true
URxvt.iso14755: false

URxvt.background: #282828
URxvt.foreground: #ebdbb2

! Black + DarkGrey
URxvt.color0:  #282828
URxvt.color8:  #928374
! DarkRed + Red
URxvt.color1:  #cc241d
URxvt.color9:  #fb4934
! DarkGreen + Green
URxvt.color2:  #98971a
URxvt.color10: #b8bb26
! DarkYellow + Yellow
URxvt.color3:  #d79921
URxvt.color11: #fabd2f
! DarkBlue + Blue
URxvt.color4:  #458588
URxvt.color12: #83a598
! DarkMagenta + Magenta
URxvt.color5:  #b16286
URxvt.color13: #d3869b
! DarkCyan + Cyan
URxvt.color6:  #689d6a
URxvt.color14: #8ec07c
! LightGrey + White
URxvt.color7:  #a89984
!URxvt.color15: #ebdbb2


! Layout
/* rofi.width:                          50 */
/* rofi.lines:                          1 */
rofi.columns:                        1
rofi.color-enabled:                  true
rofi.color-normal:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-urgent:                   #282828,#d79921,#353535,#d79921,#282828
rofi.color-active:                   #282828,#ebdbb2,#353535,#458588,#ebdbb2
rofi.color-window:                   #dd282828,#458488

! Behaviour
rofi.terminal:                       urxvt
rofi.levenshtein-sort:               true
rofi.fuzzy:                          true
rofi.kb-cancel:                      Escape,Control+c
rofi.font:     DejaVu Sans Mono for Powerline 22
rofi.hide-scrollbar: true
rofi.kb-row-up: Super+k
rofi.kb-row-down: Super+j
rofi.kb-mode-next: Super+l
rofi.kb-mode-previous: Super+h
rofi.fullscreen: true
rofi.opacity: 100
rofi.fake-transparency: true
rofi.padding: 300
rofi.auto-select: false
